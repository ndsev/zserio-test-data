package comments.classic_doc.constant_comments;

/** Minimum 8-bit unsigned integer. */
const uint8 UINT8_MIN_CONSTANT = 0x00;

/**
 * Maximum 8-bit unsigned integer.
 *
 * This constant should be everywhere to get rid of magic numbers.
 *
 * @see UINT8_MIN_CONSTANT
 */
const uint8 UINT8_MAX_CONSTANT = 0xFF;
